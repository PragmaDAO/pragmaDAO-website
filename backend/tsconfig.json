{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "../node_modules/@types"],
    "types": ["node", "express", "cors", "passport", "passport-google-oauth20", "passport-github2", "express-session"]
  }
}